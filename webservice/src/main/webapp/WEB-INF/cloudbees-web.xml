<?xml version="1.0"?>
<cloudbees-web-app xmlns="http://www.cloudbees.com/xml/webapp/1">
    <!-- Application ID (formatted CB_ACCOUNT/APPNAME) -->
    <appid>gegron/biblibouille</appid>

    <!-- Extra context parameters -->
    <!--<context-param>-->
        <!--<param-name>PARAM_NAME</param-name>-->
        <!--<param-value>PARAM_VALUE</param-value>-->
    <!--</context-param>-->

    <!-- DataSources (use names referenced via <resource-ref> in WEB-INF/web.xml) -->
    <resource name="jdbc/DATASOURCE_NAME" auth="Container" type="javax.sql.DataSource">
        <param name="username" value="DB_USER" />
        <param name="password" value="DB_PASS" />
        <param name="url" value="jdbc:mysql://DB_HOST:3306/DB_NAME" />
    </resource>
</cloudbees-web-app>